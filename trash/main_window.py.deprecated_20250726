        self.dxf_parser = DXFParser()
        self._data_adapter = None  # å»¶è¿Ÿåˆå§‹åŒ–
        self.status_manager = StatusManager()
        
        # ä½¿ç”¨ä¾èµ–æ³¨å…¥è·å–æ•°æ®æœåŠ¡ï¼Œè€Œä¸æ˜¯ç›´æ¥åˆ›å»ºå…¨å±€çŠ¶æ€
        from src.core.data_service_interface import get_data_service
        self.data_service = get_data_service()
        
        # ä¸ºäº†å…¼å®¹æ€§ï¼Œæš‚æ—¶ä¿ç•™shared_data_managerè®¿é—®ï¼ˆå°†é€æ­¥ç§»é™¤ï¼‰
        from src.core.shared_data_manager import SharedDataManager
        self.shared_data_manager = SharedDataManager()
        
        # æ‰‡å½¢åŒºåŸŸç®¡ç†å™¨ - ä½¿ç”¨å…±äº«æ•°æ®ç®¡ç†å™¨ä¸­çš„å®ä¾‹ï¼Œç¡®ä¿æ˜¯åŒä¸€ä¸ª
        self.sector_manager = self.shared_data_manager.unified_adapter
        
        # äº§å“ç®¡ç†
