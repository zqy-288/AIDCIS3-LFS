        self.goto_report_btn.setMinimumHeight(40)
        self.goto_report_btn.setFont(button_font)
        # ä½¿ç”¨ä¸»é¢˜ç®¡ç†å™¨çš„è­¦å‘Šè‰²æ ·å¼
        self.goto_report_btn.setObjectName("WarningButton")
