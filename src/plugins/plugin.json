{
    "name": "ExampleUIWidget",
    "version": "1.0.0",
    "description": "示例UI组件插件，演示插件系统的基本功能",
    "author": "AI-2 UI层重构工程师",
    "plugin_type": "ui_component",
    "entry_point": "example_ui_widget_plugin",
    "dependencies": [],
    "api_version": "1.0.0",
    "min_app_version": "1.0.0",
    "permissions": ["ui.create_widget", "ui.theme_access"],
    "config_schema": {
        "type": "object",
        "properties": {
            "input_text": {
                "type": "string",
                "description": "默认输入文本"
            },
            "counter": {
                "type": "integer",
                "description": "计数器初始值",
                "default": 0
            },
            "notes": {
                "type": "string",
                "description": "默认笔记内容"
            }
        }
    },
    "tags": ["example", "ui", "widget", "demo"],
    "enabled": true,
    "auto_start": true,
    "priority": 100,
    "ui_type": "widget",
    "capabilities": ["configurable", "themeable", "resizable"],
    "default_position": "center",
    "icon": "widget.png",
    "menu_text": "示例组件",
    "tooltip": "一个演示用的UI组件插件"
}