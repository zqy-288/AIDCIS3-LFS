{
  "id": "enhanced_dxf_processor",
  "name": "增强DXF处理器",
  "version": "1.2.0",
  "description": "提供高级DXF文件解析和孔位检测功能，支持多种DXF版本和复杂几何检测",
  "author": "AI-1 架构重构工程师",
  "homepage": "https://github.com/aidcis/enhanced-dxf-plugin",
  "license": "MIT",
  "tags": ["dxf", "cad", "geometry", "hole-detection", "parser"],
  
  "entry_point": "enhanced_dxf_plugin.EnhancedDXFPlugin",
  "python_requires": ">=3.8",
  "platform": ["win32", "linux", "darwin"],
  
  "dependencies": [],
  "optional_dependencies": ["numpy", "scipy"],
  "conflicts": ["basic_dxf_parser"],
  "provides": ["dxf_parser", "hole_detector", "geometry_validator"],
  "requires": ["config_manager", "logger"],
  
  "min_core_version": "1.0.0",
  "max_core_version": "2.0.0",
  "api_version": "1.0.0",
  
  "permissions": [
    "file_read",
    "inter_plugin_communication",
    "extension_points"
  ],
  "priority": 10,
  "enabled": true,
  "auto_start": true,
  "hot_reload": true,
  "sandboxed": false,
  
  "config_schema": {
    "type": "object",
    "properties": {
      "hole_radius_tolerance": {
        "type": "number",
        "minimum": 0.001,
        "maximum": 1.0,
        "default": 0.1,
        "description": "孔径检测容差"
      },
      "min_hole_radius": {
        "type": "number",
        "minimum": 0.1,
        "maximum": 10.0,
        "default": 0.5,
        "description": "最小孔径"
      },
      "max_hole_radius": {
        "type": "number",
        "minimum": 10.0,
        "maximum": 100.0,
        "default": 50.0,
        "description": "最大孔径"
      },
      "angle_precision": {
        "type": "number",
        "minimum": 0.1,
        "maximum": 10.0,
        "default": 1.0,
        "description": "角度精度"
      },
      "enable_advanced_detection": {
        "type": "boolean",
        "default": true,
        "description": "启用高级检测算法"
      },
      "cache_parsed_files": {
        "type": "boolean",
        "default": true,
        "description": "缓存解析结果"
      }
    },
    "required": ["hole_radius_tolerance", "min_hole_radius", "max_hole_radius"]
  },
  
  "extension_points": ["dxf_parser", "dxf_processor", "business_rule"],
  "extension_implementations": {
    "dxf_parser": "can_parse,parse_file,get_supported_versions",
    "dxf_processor": "process_entities,extract_holes,validate_geometry",
    "business_rule": "validate_hole_data,calculate_metrics,apply_business_logic"
  }
}